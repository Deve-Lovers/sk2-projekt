{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es2021"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "petCare/*": [
        "./*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "extends": "@tsconfig/react-native/tsconfig.json"
}
